{
  "extends": "airbnb",
  "settings": {
    "import/resolver": {
      "webpack":{
         "config": 'webpack.config.client.js'
      }
    }
  },
  "parserOptions": {
    "ecmaFeatures": {
      experimentalObjectRestSpread: true,
    }
  },
  "globals": {
    "document": true,
    "window": true,
  },
  "rules": {
    "no-underscore-dangle": 'off'
  }
}
